<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_list_parting_line_bg" />
            <stroke android:width="1dp" android:color="@color/color_list_parting_line_bg" />
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_grey_666666" />
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
</selector>